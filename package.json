    "eslint": "^9.17.0",
    "eslint-config-next": "^15.1.2",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.4.2",
    "semantic-release": "^24.2.0",
    "supabase": "^2.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "vitest": "^3.0.7"
  },
  "dependencies": {
    "@supabase/ssr": "^0.5.2",
